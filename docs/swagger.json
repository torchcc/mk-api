                        "name": "level",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "套餐类别id",
                        "name": "category_id",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "价格区间左值, 0表示最小",
                        "name": "min_price",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "价格区间左值, 0表示无上限",
                        "name": "max_price",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "适用人群 0-不限 1-男士 2-女未婚 3-女已婚",
                        "name": "target",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "高发疾病 0-不限 1-食物不耐受检测，2-骨关节疾病体检 3-健康防癌体检 4-幽门螺旋杆菌检测 5-甲状腺检测 6-糖尿病检测",
                        "name": "disease",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "优先排序 0-默认排序，1-低价优先 2 高价优先",
                        "name": "order_by",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/middleware.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "allOf": [
                                                {
                                                    "$ref": "#/definitions/dto.PaginateListOutput"
                                                },
                                                {
                                                    "type": "object",
                                                    "properties": {
                                                        "list": {
                                                            "type": "array",
                                                            "items": {
                                                                "$ref": "#/definitions/dto.ListPackageOutputEle"
                                                            }
                                                        }
                                                    }
                                                }
